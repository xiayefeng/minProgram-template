.action-sheet-wrap .item-wrap, .action-sheet-wrap .top-title .close-btn {
  position: absolute;
}

.action-sheet-wrap .top-title {
  position: relative;
}

.action-sheet-wrap {
  position: fixed;
}

@font-face {
  font-family: "iconfont";
  src: url("data:application/x-font-woff;charset=utf-8;base64,") format("woff");
}
.iconfont, .action-sheet-wrap .top-title .ali-icon {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
}

.icon-guanbi:before {
  content: "\e711";
}

.action-sheet-wrap {
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.4);
  z-index: 5;
}
.action-sheet-wrap .item-wrap {
  left: 0;
  width: 100%;
  transition: transform 0.3s;
}
.action-sheet-wrap .item-wrap-bottom {
  bottom: 0;
  transform: translate3d(0, 100%, 0);
}
.action-sheet-wrap .item-wrap-top {
  top: 0;
  transform: translate3d(0, -100%, 0);
}
.action-sheet-wrap .item-wrap.item-show {
  transform: translate3d(0, 0, 0);
  transition: transform 0.3s;
}
.action-sheet-wrap .top-title {
  text-align: center;
  font-size: 32rpx;
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
  border-bottom: 1px solid #e8e8ed;
}
.action-sheet-wrap .top-title .close-btn {
  right: 20rpx;
  top: 50%;
  width: 60rpx;
  height: 60rpx;
  transform: translateY(-50%);
  line-height: 60rpx;
}
.action-sheet-wrap .top-title .ali-icon {
  font-size: 42rpx;
}

.warn-btn {
  background-color: #e4393c;
  color: #fff;
}

